{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "zsglobal": {
            "type": "object",
            "properties": {
                "affinity": {
                    "type": "object"
                },
                "autoscaling": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "maxReplicas": {
                            "type": "integer"
                        },
                        "memoryAutoScalingEnabled": {
                            "type": "boolean"
                        },
                        "minReplicas": {
                            "type": "integer"
                        },
                        "targetCPUUtilizationPercentage": {
                            "type": "integer"
                        },
                        "targetMemoryUtilizationPercentage": {
                            "type": "integer"
                        }
                    }
                },
                "commandline": {
                    "type": "object",
                    "properties": {
                        "args": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "fullnameOverride": {
                    "type": "string"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "multiConnector": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "secretIdentifier": {
                            "type": "string"
                        }
                    }
                },
                "nameOverride": {
                    "type": "string"
                },
                "namespace": {
                    "type": "string"
                },
                "nodeSelector": {
                    "type": "object"
                },
                "podAnnotations": {
                    "type": "object"
                },
                "provisionkey": {
                    "type": "object",
                    "properties": {
                        "value": {
                            "type": "string"
                        }
                    }
                },
                "replicaCount": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object",
                    "properties": {
                        "limits": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        },
                        "requests": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "scc": {
                    "type": "string"
                },
                "secretIdentifier": {
                    "type": "string"
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "capabilities": {
                            "type": "object",
                            "properties": {
                                "add": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "securityContextConstraints": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "create": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        },
                        "securityContextConstraints": {
                            "type": "object",
                            "properties": {
                                "allowedCapabilities": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "runAsUser": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "seLinuxContext": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "users": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "tolerations": {
                    "type": "array"
                }
            }
        }
    }
}
